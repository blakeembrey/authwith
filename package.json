{
  "name": "authwith",
  "version": "0.0.0",
  "description": "Re-usable authentication library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" --type-check --project tsconfig.json",
