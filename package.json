  "bugs": {
    "url": "https://github.com/blakeembrey/authwith/issues"
  },
  "homepage": "https://github.com/blakeembrey/authwith",
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.12",
    "jest": "^19.0.0",
    "rimraf": "^2.5.4",
    "tslint": "^5.1.0",
    "tslint-config-standard": "^5.0.2",
    "typescript": "^2.2.1",
    "typings": "^2.1.0"
  },
  "dependencies": {
